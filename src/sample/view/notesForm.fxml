      </JFXButton>
      <JFXTextArea fx:id="notesTextArea" focusColor="BLACK" layoutX="150.0" layoutY="141.0" prefHeight="220.0" prefWidth="540.0" promptText="Enter notes . . ." style="-fx-background-color: #eeeeee;" unFocusColor="WHITE" />
